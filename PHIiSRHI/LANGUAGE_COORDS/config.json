{
  "schema_version": "1.0",
  "system_name": "Multi-Agent Windows Coordination System",
  "version": "1.0.0",
  "created_date": "2024-01-01T00:00:00Z",
  "environment": {
    "os": "Windows 10/11",
    "powershell_version": "7+",
    "autohotkey_version": "2.0",
    "architecture": "x64"
  },
  "paths": {
    "config_dir": "./config",
    "logs_dir": "./logs",
    "cache_dir": "./cache",
    "temp_dir": "./temp",
    "scripts_dir": "./scripts"
  },
  "performance": {
    "message_timeout_ms": 500,
    "max_retries": 3,
    "snapshot_interval_turns": 10,
    "max_snapshots": 10,
    "cleanup_interval_minutes": 30
  },
  "coordination": {
    "turn_duration_ms": 1000,
    "handoff_timeout_ms": 500,
    "unstall_detection_turns": 3,
    "deadlock_detection_enabled": true
  },
  "messaging": {
    "compression_enabled": true,
    "encryption_enabled": false,
    "max_message_size_bytes": 1048576,
    "message_retention_minutes": 60
  },
  "agents": {
    "STRYK": {
      "enabled": true,
      "startup_priority": 1,
      "resource_limits": {
        "memory_mb": 128,
        "cpu_percent": 25
      }
    },
    "DC": {
      "enabled": true,
      "startup_priority": 2,
      "resource_limits": {
        "memory_mb": 256,
        "cpu_percent": 50
      }
    },
    "VSCC": {
      "enabled": true,
      "startup_priority": 3,
      "resource_limits": {
        "memory_mb": 512,
        "cpu_percent": 75
      }
    }
  }
}