{
  "schema_version": "1.0",
  "enabled": true,
  "target_reduction_percent": 66,
  "target_tokens_per_turn": 50,
  "baseline_tokens_per_turn": 150,
  "rules": {
    "shorthand_replacements": {
      "the": "",
      "and": "&",
      "for": "4",
      "to": "2",
      "you": "u",
      "with": "w/",
      "without": "w/o",
      "window": "win",
      "application": "app",
      "function": "fn",
      "variable": "var",
      "execute": "exec",
      "process": "proc",
      "message": "msg",
      "coordination": "coord",
      "automation": "auto",
      "management": "mgmt",
      "optimization": "opt",
      "validation": "val",
      "information": "info",
      "configuration": "config",
      "parameter": "param",
      "response": "resp",
      "request": "req",
      "error": "err",
      "success": "ok",
      "failure": "fail",
      "return": "ret",
      "argument": "arg",
      "collection": "coll",
      "element": "elem",
      "component": "comp",
      "interface": "iface",
      "implementation": "impl",
      "specification": "spec",
      "documentation": "doc",
      "directory": "dir",
      "filename": "file",
      "extension": "ext",
      "identifier": "id",
      "reference": "ref",
      "pointer": "ptr",
      "address": "addr",
      "value": "val",
      "boolean": "bool",
      "integer": "int",
      "string": "str",
      "number": "num",
      "array": "arr",
      "object": "obj",
      "property": "prop",
      "method": "meth",
      "class": "cls",
      "instance": "inst",
      "constructor": "ctor",
      "destructor": "dtor",
      "handler": "hdlr",
      "listener": "lstnr",
      "callback": "cb",
      "promise": "prom",
      "async": "async",
      "await": "await",
      "module": "mod",
      "package": "pkg",
      "library": "lib",
      "framework": "fw",
      "template": "tmpl",
      "pattern": "pat",
      "algorithm": "algo",
      "structure": "struct",
      "data": "data",
      "metadata": "meta",
      "content": "cnt",
      "resource": "res",
      "service": "svc",
      "client": "cli",
      "server": "srv",
      "protocol": "proto",
      "connection": "conn",
      "session": "sess",
      "transaction": "tx",
      "operation": "op",
      "action": "act",
      "event": "evt",
      "trigger": "trig",
      "condition": "cond",
      "expression": "expr",
      "statement": "stmt",
      "declaration": "decl",
      "definition": "def",
      "assignment": "assign",
      "comparison": "comp",
      "iteration": "iter",
      "recursion": "recur",
      "inheritance": "inherit",
      "polymorphism": "poly",
      "encapsulation": "encap",
      "abstraction": "abstract"
    },
    "symbol_replacements": {
      "equals": "==",
      "not_equals": "!=",
      "greater_than": ">",
      "less_than": "<",
      "greater_equal": ">=",
      "less_equal": "<=",
      "and_logic": "&&",
      "or_logic": "||",
      "not_logic": "!",
      "assignment": "=",
      "addition": "+",
      "subtraction": "-",
      "multiplication": "*",
      "division": "/",
      "modulo": "%",
      "increment": "++",
      "decrement": "--",
      "arrow": "->",
      "scope": "::",
      "namespace": "::",
      "member": ".",
      "pipe": "|",
      "redirect": ">",
      "append": ">>",
      "background": "&",
      "null": "null",
      "undefined": "undef",
      "true": "true",
      "false": "false"
    },
    "compression_rules": {
      "remove_articles": true,
      "remove_punctuation": false,
      "lowercase_all": true,
      "remove_whitespace": false,
      "abbreviate_common": true,
      "use_symbols": true,
      "json_compression": true
    }
  },
  "optimization_strategies": {
    "batch_operations": true,
    "minimize_handshakes": true,
    "compress_context": true,
    "cache_repeated": true,
    "predictive_sending": false
  },
  "monitoring": {
    "track_usage": true,
    "report_violations": true,
    "auto_optimize": true,
    "alerts_threshold": 60
  },
  "fallback": {
    "graceful_degradation": true,
    "minimal_mode_tokens": 75,
    "emergency_mode_tokens": 100
  }
}